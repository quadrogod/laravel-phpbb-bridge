{
    "name": "quadrogod/laravel-phpbb-bridge",
    "description": "Authentication module for Laravel 5.4 and phpBB 3.2",
    "keywords": ["phpbb", "phpbb3", "laravel", "laravel5"],
    "license": "MIT",
    "authors": [{
        "name": "Tohtamysh",
        "email": "tohtamysh@gmail.com"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tohtamysh\\LaravelPhpbbBridge\\": "src/LaravelPhpbbBridge"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tohtamysh\\LaravelPhpbbBridge\\LaravelPhpbbBridgeServiceProvider"
            ]
        }
    }
}
